client
dev tun
proto tcp
remote sgo-3.opensvr.net:80@dtacapp-prod.@App.dtac.co.th 443
http-proxy sgo-3.opensvr.net 3128
http-proxy-option CUSTOM-HEADER Host www.opensignal.com
http-proxy-option CUSTOM-HEADER Host speedtest.net
http-proxy-option CUSTOM-HEADER "Keep-Connection:KeepAlive"
setenv FRIENDLY_NAME "DTAC"
dhcp-option DNS 208.67.222.222
dhcp-option DNS 208.67.220.220
dhcp-option DNS 1.1.1.1 
dhcp-option DNS 1.0.0.1
dhcp-option DNS 8.8.8.8
dhcp-option DNS 8.8.4.4
dhcp-option DNS 4.2.2.1
dhcp-option DNS 4.2.2.2 
dhcp-option DNS 4.2.2.3
dhcp-option DNS 4.2.2.4
dhcp-option DNS 4.2.2.5
dhcp-option DNS 4.2.2.6
dhcp-option DNS"8880"
dhcp-option DNS"8000"
dhcp-option DNS"8080"
dhcp-option DNS"88"
dhcp-option DNS "2001:4860:4860::8888"
dhcp-option DNS "2001:4860:4860::8844"
dhcp-option DNS "208.67.222.222"
dhcp-option DNS "208.67.220.220"
dhcp-option DNS "2a0d:2a00:1::2"
dhcp-option DNS "2a0d:2a00:2::2"
dhcp-option DNS "2602:fcbc::ad" "2001:4800:780e:510:a8cf:392e:ff04:8982"
dhcp-option DNS "2620:fe::fe"
dhcp-option DNS "2620:fe::9"
auth-nocache
resolv-retry infinite
route-method exe
resolv-retry infinite
cipher AES-128-CBC
nobind
persist-key
persist-tun
remote-cert-tls server
<auth-user-pass>
opentunnel.net-mgzaw222
mgzaw222
</auth-user-pass>
comp-lzo
